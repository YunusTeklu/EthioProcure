@model IEnumerable<EthioProcure.Models.Bid>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="blackness">
    <h3>Bids for @ViewBag.projectName</h3>

    <table class="table table-hover table-striped table-responsive">
        <tr>

            <th>
                Contractor that submitted their Bids
            </th>

        </tr>

        @foreach (var item in Model)
        {
            <tr onclick="window.location='@Url.Action("Details", new { id= item.BidId} )'">

                <td>
                    @Html.DisplayFor(modelItem => item.contractor.organizationName)
                </td>

            </tr>
        }

    </table>
</div>

